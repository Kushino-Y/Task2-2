<h1>Editing Book</h1>
<h3><%= notice %></h3>
<%= form_with model:@book, local:true do |f| %>
  <h4>Title</h4>
  <%= f.text_field :title %>
  <h4>Body</h4></h4>
  <%= f.text_area :body %>
  <%= f.submit 'Update Book' %>
<% end %>
<%= link_to "Show", "/books/#{@book.id}" %>
<%= link_to "Back", "/books/new" %>